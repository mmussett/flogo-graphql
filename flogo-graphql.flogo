                    "value": "{\"$schema\":\"http://json-schema.org/draft-04/schema#\",\"type\":\"object\",\"properties\":{\"Devices\":{\"type\":\"array\",\"items\":{}},\"options\":{\"type\":\"object\",\"properties\":{}}}}",
                    "fe_metadata": "{\n    \"Devices\":[],\n    \"options\": {}\n}"
                  }
